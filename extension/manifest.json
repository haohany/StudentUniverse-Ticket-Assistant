{
  "name": "StudentUniverse Ticket Assistant",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Get best plane ticket deal from StudentUniverse",
  "browser_action": {
    "default_icon": {
      "19": "img/icon19.png",
      "38": "img/icon38.png"
    },
    "default_title": "StudentUniverse - click for details"
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "background": {
    "scripts": [
      "js/bg.js"
    ]
  },
  "permissions": [
    "<all_urls>",
    "tabs"
  ]
}
